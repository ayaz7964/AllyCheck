{
  "name": "allycheck",
  "version": "1.0.0",
  "description": "AllyCheck - Automated accessibility testing for websites with AI-powered fix recommendations",
  "keywords": [
    "accessibility",
    "a11y",
    "wcag",
    "testing",
    "axe-core",
    "disability",
    "inclusion",
    "gemini-ai"
  ],
  "author": "AllyCheck Team",
  "license": "MIT",
